<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
     <servlet-name>GetLocationCall</servlet-name>
     <servlet-class>com.anjali.servlet.GetLocationCall</servlet-class>
     <load-on-startup>1</load-on-startup>
 </servlet>
 <servlet-mapping>
     <servlet-name>GetLocationCall</servlet-name>
     <url-pattern>/GetLocation/*</url-pattern>
 </servlet-mapping>
  <servlet>
     <servlet-name>GetWeatherConditionsCall</servlet-name>
     <servlet-class>com.anjali.servlet.GetWeatherConditionsCall</servlet-class>
     <load-on-startup>2</load-on-startup>
 </servlet>
 <servlet-mapping>
     <servlet-name>GetWeatherConditionsCall</servlet-name>
     <url-pattern>/GetWeatherConditions/*</url-pattern>
 </servlet-mapping>
</web-app>
